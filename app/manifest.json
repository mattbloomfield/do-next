{
    "name": "Do Next",
    "version": "2.2.1",
    "description": "Figure out quickly what the next thing on your schedule should be. Don't get lost on Twitter!",
    "permissions": [
        "storage"
    ],
    "options_page": "options/options.html",
    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "images/logo16.png",
            "32": "images/logo32.png",
            "48": "images/logo48.png",
            "128": "images/logo128.png"
        }
    },
    "icons": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
    },
    "manifest_version": 2
}